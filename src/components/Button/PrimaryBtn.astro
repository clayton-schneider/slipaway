---
interface Props {
  to: string;
  text: string;
  classes?: string;
}
const { to, text, classes } = Astro.props;
---

<a
  href={to}
  class={classes +
    " rounded-full inline-block bg-orange font-bold text-white border-2 transition-all duration-300 border-orange py-3 px-6 hover:scale-105"}
  >{text}</a
>
